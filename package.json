{
  "name": "curriculum-vitae",
  "version": "1.0.0",
  "description": "negiseijinの職務経歴書",
  "repository": "git@github.com:negiseijin/Curriculum-vitae.git",
  "author": "negiseijin <satoru6221@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "lint": "textlint docs/README.md",
    "lint--fix": "textlint --fix docs/README.md",
    "build:pdf": "md-to-pdf docs/README.md --config-file ./pdf-configs/config.js",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(md)": [
      "yarn lint--fix"
    ]
  },
  "dependencies": {
    "husky": "^7.0.1",
    "md-to-pdf": "^4.0.0",
    "textlint": "^12.0.2"
  },
  "devDependencies": {
    "@proofdict/textlint-rule-proofdict": "^3.1.2",
    "lint-staged": "^11.1.2",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0"
  }
}
